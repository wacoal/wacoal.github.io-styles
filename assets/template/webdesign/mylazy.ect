<% block 'title': %>
mylazy
<% end %>

<% block 'css': %>
<style>
.main {
display: block;
}
.lists {
display: block;
}
.lists__item {
display: block;
margin: 0 auto 16px;
}
.pic {
display: block;
max-width: 100%;
}
</style>
<% end %>

<% extend 'partials/layout.ect' %>

<div class="main">
	<ul class="lists">
		<li class="lists__item js-mylazy">
			<img class="pic" data-pic="" src="data:image/gif;base64," alt="">
		</li>
	</ul>
</div>
<script>
$(function(){
  var window_width = $(window).width();
  var window_height = $(window).height();
  var lazy_pic = (".js-mylazy img");

  $(window).on("load resize scroll",function(){
    window_width = $(window).width();
    window_height = $(window).height();

    if(lazy_pic){

        $(lazy_pic).each(function() {

          var lazy_pic_height = $(this).height();


          console.log(lazy_pic_height);

      	});
    }

  });
});

</script>
